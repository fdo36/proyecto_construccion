<h1>Listing access_rights</h1>

<table>
  <tr>
    <th>Model</th>
    <th>Right</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @access_rights.each do |access_right| %>
  <tr>
    <td><%= access_right.model %></td>
    <td><%= access_right.right %></td>
    <td><%= link_to 'Show', access_right %></td>
    <td><%= link_to 'Edit', edit_access_right_path(access_right) %></td>
    <td><%= link_to 'Destroy', access_right, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

