<p id="notice"><%= notice %></p>
<div class="headContent">
  <h1>Ver Packing Pallet Final</h1>
</div>
<div class="box">
  <%= link_to 'Volver', final_packing_pallets_path , :class => "button blue alignright" %>
  <p>
    <b>Especie:</b>
    <%= Kind.find(@final_packing_pallet.kind_id).name %>
  </p>

  <p>
    <b>Variedad:</b>
    <%= Variety.find(@final_packing_pallet.variety_id).name %>
  </p>

  <p>
    <b>Calidad:</b>
    <%= Quality.find(@final_packing_pallet.quality_id).name %>
  </p>

  <p>
    <b>Pack Packing:</b>
    <%= @final_packing_pallet.pack_packing_id %>
  </p>

  <p>
    <b>Formato:</b>
    <%= @final_packing_pallet.format_id %>
  </p>

  <p>
    <b>Cantidad:</b>
    <%= @final_packing_pallet.quantity %>
  </p>

  <p>
    <b>Peso Neto:</b>
    <%= @final_packing_pallet.net_weight %>
  </p>
</div>
<div class="foot-box">
  <%= link_to 'Editar', edit_final_packing_pallet_path(@final_packing_pallet) , :class => "button green alignright" %>
</div>