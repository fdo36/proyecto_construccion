<p id="notice"><%= notice %></p>
<div class="headContent">
  <h1>Ver Proveedor</h1>
</div>
<div class="box">
<%= link_to 'Volver', providers_path  , :class => "button blue alignright" %>
<p>
  <b>Nombre:</b>
  <%= @provider.name %>
</p>

<p>
  <b>Rut:</b>
  <%= @provider.rut %>
</p>

<p>
  <b>Telefono:</b>
  <%= @provider.phone %>
</p>

<p>
  <b>Email:</b>
  <%= @provider.email %>
</p>

<p>
  <b>Direccion:</b>
  <%= @provider.address %>
</p>

<p>
  <b>Comuna:</b>
  <%= @provider.commune_id %>
</p>

<p>
  <b>Contacto:</b>
  <%= @provider.contact %>
</p>

<p>
  <b>Activo:</b>
  <%= @provider.active %>
</p>

</div>
<div class="foot-box">
  <%= link_to 'Editar', edit_provider_path(@provider) , :class => "button green alignright" %>
</div>

