<div class="headContent">
    <h1>Lista de Túneles de Congelado</h1>
</div>
<div class="box">
    <div class="newTopBanner">
        <%= link_to 'Nuevo Túnel de Congelado', new_frozen_tunnel_io_path , :class => "button blue alignright" %>
    </div>

    <p id="notice" class="alignleft"><%= notice %> </p>

    <table cellpadding="0" cellspacing="0" border="0" class="display datatable">
      <thead>
          <tr>
            <th>Order number</th>
            <th>Tunnel</th>
            <th>Tunnel temperature</th>
            <th>Packing pallet temperature</th>
          </tr>
      </thead>
      <tbody>
        <% @frozen_tunnel_ios.each do |frozen_tunnel_io| %>
          <tr>
            <td><%= frozen_tunnel_io.order_number %></td>
            <td><%= frozen_tunnel_io.tunnel_id %></td>
            <td><%= frozen_tunnel_io.tunnel_temperature %></td>
            <td><%= frozen_tunnel_io.packing_pallet_temperature %></td>
            <td><%= link_to 'Show', frozen_tunnel_io %>
            <%= link_to 'Edit', edit_frozen_tunnel_io_path(frozen_tunnel_io) %>
            <%= link_to 'Destroy', frozen_tunnel_io, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

</div>