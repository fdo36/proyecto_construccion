  function validate_integer(strValue)
  {
      
      
      var objRegExp  = /(^-?\d\d*$)/;  
      var result = objRegExp.test(strValue);
      if(result)
      {
        return true;
      }
      return false;

  }

  function check_decimal(inputtxt)   
