    var decimal=  /^[-+]?[0-9]+\.[0-9]+$/; 
    if(inputtxt.match(decimal) || validate_integer(inputtxt))   
    {   
      alert('Correct, try another...')  
      return true;  
    }  
    else  
    {   
      alert('Wrong...!')  
      return false;  
    }  
  } 


  
