<div class="headContent">
  <h1>Lista de Pack Packing</h1>
</div>
<div class="box">
  <div class="newTopBanner">
    <%= link_to 'Nuevo Pack packing', new_pack_packing_path, :class => "button blue alignright" %>
  </div>

  <p id="notice" class="alignleft"><%= notice %> </p>

  <table cellpadding="0" cellspacing="0" border="0" class="display datatable">
    <thead>
      <tr>
        <th>Nombre</th>
        <th colspan="3" style="width:20% !important;">Acciones</th>
      </tr>
    </thead>
    <tbody>
      <% @pack_packings.each do |pack_packing| %>
        <tr>
          <td><%= pack_packing.name %></td>
          <td><%= link_to 'Ver', pack_packing %>
          <%= link_to 'Editar', edit_pack_packing_path(pack_packing) %>
          <%= link_to 'Eliminar', pack_packing, method: :delete, data: { confirm: 'Â¿Esta seguro que desea eliminar este pack packing?' } %></td>
        </tr>
      <% end %>
    </tbody>  
  </table>
</div>


