<div class="headContent">
  <h1>Lista de Especies de <%= @producer.name %></h1>
</div>
<div class="box">
  <table cellpadding="0" cellspacing="0" border="0" class="display datatable">
    <thead>
      <tr>
        <th style="width:20% !important;">Nombre</th>
      </tr>
    </thead>
    <tbody>
      <% kinds = @producer.kinds %>
      <% kinds.each do |kind| %>
        <tr>
          <td><%= kind.name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>